https://www.codechef.com/INOIPRAC/problems/INOI1301

```cpp
#include <bits/stdc++.h>
#define ll long long
using namespace std;

const ll max_N = 1000010;
ll f[max_N];
ll b[max_N];
ll A[max_N];

ll solve(ll A[], ll n, ll k) {
    ll M=LLONG_MIN;

    f[k]=0; f[k+1]=A[k+1];
    for (ll i=k+2; i<=n; i++) {
        f[i] = max(f[i-1]+A[i], f[i-2]+A[i]);
    }
    b[1]=A[1]; b[2]=A[1]+A[2];
    for (ll i=3; i<=n; i++) {
        b[i]=max(b[i-1]+A[i], b[i-2]+A[i]);
    }
    for (ll i=k; i<=n; i++) {
        M = max(M, f[i]+b[i]-A[i]);
    }
    return M;
}

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    ll n, k;
    cin >> n >> k;

    for (ll i=1; i<=n; i++) {
        cin >> A[i];
    }

    cout <<  solve(A, n, k);

    return 0;
}
```
